	file = {arXiv Fulltext PDF:/home/bsg/Zotero/storage/E9MXGBJI/Shibata and Hotokezaka - 2019 - Merger and Mass Ejection of Neutron-Star Binaries.pdf:application/pdf;arXiv.org Snapshot:/home/bsg/Zotero/storage/WT2A6YNM/1908.html:text/html}
}

