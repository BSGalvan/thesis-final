	keywords = {Astrophysics - High Energy Astrophysical Phenomena, General Relativity and Quantum Cosmology},
	file = {arXiv Fulltext PDF:/home/bsg/Zotero/storage/SKACPHNY/Kastha et al. - 2020 - Imprints of the redshift evolution of double neutr.pdf:application/pdf;arXiv.org Snapshot:/home/bsg/Zotero/storage/D7EV5K4J/1801.html:text/html}
}

@article{mohan_detectability_2019,
	title = {Detectability of Electromagnetic counterparts from Neutron Star mergers: prompt emission vs afterglow},
	url = {http://arxiv.org/abs/1912.09436},
	shorttitle = {Detectability of Electromagnetic counterparts from Neutron Star mergers},
	abstract = {Electromagnetic observations of the first binary Neutron Star ({BNS}) merger {GW}170817 has established that relativistic jets can be successfully launched in {BNS} mergers. Typically, such jets produce emission in two phases: \${\textbackslash}gamma\$-ray prompt emission and multi-wavelength afterglow. Due to relativistic de-boosting, the detectability of both these counterparts is dependent on the angle (\${\textbackslash}theta\_v\$) between the observer's line of sight and the jet axis. We compare the detectability of prompt and afterglow emission from off-axis jets, assuming standard detector thresholds. We find that for top-hat jets, afterglow is a more potential counterpart than the prompt emission even with unfavorable afterglow parameters. For structured jets with a Gaussian profile, prompt emission is more promising than the afterglows at extreme viewing angles, under the assumption that the energy emitted in the prompt phase equals the kinetic energy of the outflow. Assuming a Gaussian jet profile, we forecast the population of \${\textbackslash}gamma\$-ray detections and find that extreme viewing angle events like {GRB}170817A will be rare. In our simulated sample, the observed isotropic equivalent energy in \${\textbackslash}gamma\$-rays is moderately correlated with the viewing angle, such that a low \$E\_\{{\textbackslash}rm iso, {\textbackslash}gamma\}\$ is almost always associated with a high off-axis viewing angle.},
	journaltitle = {{arXiv}:1912.09436 [astro-ph]},
	author = {Mohan, S. and Saleem, M. and Resmi, L.},
	urldate = {2020-10-28},
	date = {2019-12-19},
	eprinttype = {arxiv},
	eprint = {1912.09436},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	file = {arXiv Fulltext PDF:/home/bsg/Zotero/storage/6YJPM2TF/Mohan et al. - 2019 - Detectability of Electromagnetic counterparts from.pdf:application/pdf;arXiv.org Snapshot:/home/bsg/Zotero/storage/3EBR9USB/1912.html:text/html}
}

@article{saleem_prospects_2020,
	title = {Prospects of joint detections of neutron star mergers and short-{GRBs} with Gaussian structured jets},
	volume = {493},
	issn = {0035-8711, 1365-2966},
	url = {http://arxiv.org/abs/1905.00314},
	doi = {10.1093/mnras/staa303},
	abstract = {{GW}170817 was the first ever joint detection of gravitational waves ({GW}) from a binary neutron star ({BNS}) merger with the detections of short \${\textbackslash}gamma\$-ray burst ({SGRB}) counterparts. Analysis of the multi-band afterglow observations of over more than a year revealed that the outflow from the merger end-product was consistent with structured relativistic jet models with the core of the jet narrowly collimated to half opening angles \${\textbackslash}sim5\$ deg. In this work, assuming all the {BNS} mergers produce Gaussian structured jets with properties as inferred for {GW}170817, we explore the prospects of joint detections of {BNS} mergers and prompt \${\textbackslash}gamma-\$ray emission, expected during the current and upcoming upgrades of {LIGO}-Virgo-{KAGRA} detectors. We discuss three specific observational aspects: 1) the distribution of detected binary inclination angles 2) the distance reach and 3) the detection rates. Unlike {GW}-only detections, the joint detections are greatly restricted at large inclination angles, due to the structure of the jets. We find that at lower inclination angles (say below 20 deg), the distance reach as well as the detection rates of the joint detections are limited by {GW} detectability while at larger inclinations (say above 20 deg), they are limited by the \${\textbackslash}gamma\$-ray detectability.},
	pages = {1633--1639},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Saleem, M.},
	urldate = {2020-10-28},
	date = {2020-04-01},
	eprinttype = {arxiv},
	eprint = {1905.00314},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	file = {arXiv Fulltext PDF:/home/bsg/Zotero/storage/XI7DDQFJ/Saleem - 2020 - Prospects of joint detections of neutron star merg.pdf:application/pdf;arXiv.org Snapshot:/home/bsg/Zotero/storage/NDSTJF7E/1905.html:text/html}
}

